# File managed by Puppet and will be overwritten during the next Puppet run.
# Do not make changes to this file directly.

# Basic Configuration

<%- @merged_config.each do |key, value| -%>
<%- if @psad_lowercase_configs.include? key -%>
<%= key.downcase.ljust(30) %> <%= value %>;
<%- else -%>
<%- if key == 'email_addresses' && value.is_a?(Array)-%>
<%= key.upcase.ljust(30) %> <%= value.join(",") %>;
<%- elsif key == 'aim_servers' && value.is_a?(Array)-%>
<%= key.upcase.ljust(30) %> [<%= value.join(", ") %>];
<%- else -%>
<%= key.upcase.ljust(30) %> <%= value %>;
<%- end -%>
<%- end -%>
<%- end -%>

# System Paths
<%- @merged_commands.each do |key, value| -%>
<%= key.ljust(30) %> <%= value %>;
<%- end -%>