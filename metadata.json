{
  "name": "tedivm-psad",
  "version": "1.1.2",
  "author": "tedivm",
  "summary": "PSAD Management with Puppet",
  "license": "MIT",
  "source": "https://github.com/tedivm/puppet-psad.git",
  "project_page": "https://github.com/tedivm/puppet-psad",
