# File managed by Puppet and will be overwritten during the next Puppet run.
# Do not make changes to this file directly.

<%- scope.lookupvar('psad::config::merged_options').each do |key, value| -%>
<%- if scope.lookupvar('psad::config::psad_lowercase_options').include? key -%>
<%= key.downcase.ljust(30) %> <%= value %>;
<%- else -%>
<%= key.upcase.ljust(30) %> <%= value %>;
<%- end -%>
<%- end -%>


<%- scope.lookupvar('psad::config::merged_commands').each do |key, value| -%>
<%= key.ljust(30) %> <%= value %>;
<%- end -%>